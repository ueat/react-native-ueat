{
  "name": "react-native-ueat",
  "description": "Integrate UEAT food ordering into your react-native application",
  "version": "0.1.0",
  "main": "cjs/index.js",
  "module": "es6/index.js",
  "repository": "git@github.com:UEAT/react-native-ueat.git",
  "author": "Benoit Tremblay <btremblay@ueat.io>",
  "license": "MIT",
  "files": [
    "es6",
    "cjs",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:es6 && npm run build:cjs",
    "build:clean": "rimraf es6 && rimraf cjs",
    "build:es6": "tsc",
    "build:cjs": "babel es6 -D -d cjs"
  },
  "peerDependencies": {
    "@react-native-community/async-storage": ">=1",
    "react": ">=16.9",
    "react-native": ">=0.59",
    "react-native-device-info": ">=5",
    "react-native-geolocation-service": ">=4",
    "react-native-webview": ">=8"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@react-native-community/async-storage": "^1.8.1",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.17",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react": "16.0.0",
    "react-native": "0.61.5",
    "react-native-device-info": "^5.5.3",
    "react-native-geolocation-service": "^4.0.0",
    "react-native-webview": "^8.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{md,html,css}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
